  },
  "keywords": [
    "react",
    "github",
    "feed"
  ],
  "author": "maferland",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "lint-staged": "^10.0.10",
    "microbundle": "^0.12.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
